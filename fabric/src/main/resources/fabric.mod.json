{
  "schemaVersion": 1,
  "id": "blockylib",
  "version": "${version}",
  "name": "BlockyLib",
  "description": "A Minecraft modding library that mostly used in mods of Blocky Series.",
  "authors": [
    "Block_Mrlimr267"
  ],
  "contributors": [
  ],
  "contact": {
    "homepage": "https://github.com/TCBuildMC",
    "sources": "https://github.com/TCBuildMC/BlockyLib",
    "issues": "https://github.com/TCBuildMC/BlockyLib/issues"
  },
  "license": "Apache-2.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "xyz.tcbuildmc.minecraft.mod.blockylib.fabric.BlockyLibFabric::onInitialize"
    ],
    "client": [
      "xyz.tcbuildmc.minecraft.mod.blockylib.fabric.BlockyLibFabric::onInitializeClient"
    ]
  },
  "mixins": [
    "blockylib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15",
    "fabric": "*",
    "minecraft": "1.20.x"
  }
}
